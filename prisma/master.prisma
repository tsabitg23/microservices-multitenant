generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/master-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}

model Tenant {
  id        String   @id @default(cuid())
  name      String
  slug      String   @unique
  dbUrl     String   
  createdAt DateTime @default(now())
}
